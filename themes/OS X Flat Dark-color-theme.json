		"editorGroupHeader.noTabsBackground": "#3C3F41",
		"tab.activeBackground": "#1F1F21",
		"tab.activeForeground": "#F7F7F7",
		"tab.inactiveForeground": "#F7F7F77a",
		"tab.inactiveBackground": "#1F1F217a"
