		"scrollbarSlider.background": "#000",
		"statusBar.background": "#1F1F21",
		"statusBar.foreground": "#f7f7f7",
		"activityBar.background": "#5D5F60",
		"activityBar.foreground": "#BBBBBB",
		"activityBarBadge.background": "#007acc",
		"statusBar.noFolderBackground": "#1F1F21",
		"statusBar.noFolderForeground": "#007acc",
		"statusBar.border": "#F7F7F711",
		"input.background": "#1F1F21",
		"input.foreground": "#F7F7F7",
		"sideBar.background": "#47484B",
		"sideBar.foreground": "#F7F7F7",
		"sideBarSectionHeader.background": "#F7F7F711",
		"sideBarSectionHeader.foreground": "#F7F7F7",
		"sideBarTitle.foreground": "#F7F7F7",
		"list.activeSelectionBackground": "#2D2E30",
		"list.activeSelectionForeground": "#5ac8fa",
		"list.inactiveSelectionBackground": "#3C3F41",
		"list.inactiveSelectionForeground": "#999999",
		"list.hoverBackground": "#3C3F41",
		"list.hoverForeground": "#5ac8fa",
